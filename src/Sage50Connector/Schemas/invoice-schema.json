{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JSON Schema for my JSON file format",

  "type": "object",

  "properties": {
    "Amount": {
      "type": "number",
      "description": "The sum of amounts for each line listed on the transaction "
    },
    "AmountDue": {
      "type": "number",
      "description": "Gets AmountDue "
    },
    "versions": {
      "type": "array",
      "description": "An array of version numbers.",
      "minItems": 1,
      "items": {
        "type": "number"
      }
    }
  }
}
